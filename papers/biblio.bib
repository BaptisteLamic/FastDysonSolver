@article{Rogovin1974,
	abstract = {A general theory of electrodynamic fluctuations in tunnel junctions is developed. Using standard many-body techniques we determine the quasiparticle, pair, and quasiparticle-pair current power spectra and show that these current fluctuations are simply related to the associated pieces of the junction I(V) characteristic. In addition a theory of the Josephson radiation linewidth is presented. {\textcopyright} 1974.},
